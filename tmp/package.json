{
  "name": "things-management-frontend-new",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open=true",
    "start-local": "ng serve -c local -o",
    "test": "ng test",
    "test-no-watch": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/pt_br.json --clean --sort --format namespaced-json --marker _",
    "build": "ng build",
    "build-qa": "ng build --configuration=stage",
    "build-dev": "ng build --configuration=development",
    "build-prod": "ng build --configuration production"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "~13.3.1",
    "@angular/cdk": "~13.3.2",
    "@angular/common": "~13.3.1",
    "@angular/compiler": "~13.3.1",
    "@angular/core": "~13.3.1",
    "@angular/forms": "~13.3.1",
    "@angular/localize": "^13.3.1",
    "@angular/material": "~13.3.2",
    "@angular/platform-browser": "~13.3.1",
    "@angular/platform-browser-dynamic": "~13.3.1",
    "@angular/router": "~13.3.1",
    "@azure/msal-angular": "^2.5.4",
    "@azure/msal-browser": "^2.34.0",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@mdi/font": "^3.9.97",
    "@microsoft/applicationinsights-web": "^2.4.4",
    "@ng-select/ng-select": "^8.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@ngxs-labs/dispatch-decorator": "4.0.2",
    "@ngxs/devtools-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.1",
    "angular-oauth2-oidc": "^13.0.1",
    "bootstrap": "^4.3.1",
    "chart.js": "^4.2.1",
    "jquery": "^3.6.3",
    "karma-coverage": "^2.2.0",
    "lodash": "^4.17.20",
    "messageformat": "^2.0.2",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^5.6.1",
    "ngx-mask": "^11.1.5",
    "ngx-pagination": "^5.0.0",
    "ngx-toggle-switch": "^2.0.5",
    "ngx-translate-messageformat-compiler": "^4.7.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "source-map-support": "^0.5.21",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.1",
    "@angular/cli": "~13.3.1",
    "@angular/compiler-cli": "~13.3.1",
    "@angular/language-service": "~13.3.1",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.5.16",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.3"
  },
  "lint-staged": {
    "src/**/*.{ts,json,html,scss}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "wallaby": {
    "autoDetect": true
  }
}
